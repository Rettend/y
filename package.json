{
  "name": "y",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "dev": "bun index.ts",
    "start": "bun index.ts",
    "build": "bun install",
    "bundle": "bun build --compile --minify --sourcemap index.ts --outfile app",
    "docker:build": "docker build --pull -t y-bot .",
    "docker:run": "docker run --rm --name y-container -d -p 3000:3000 --env-file .env y-bot",
    "docker:stop": "docker stop y-container",
    "lint": "eslint . --fix"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@discordjs/core": "^1.1.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@types/bun": "latest",
    "eslint": "^8.57.0"
  }
}
